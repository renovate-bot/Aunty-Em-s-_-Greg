{
  "name": "prettier",
  "version": "3.1.0-dev",
  "description": "Prettier is an opinionated code formatter",
  "bin": "./bin/prettier.cjs",
  "repository": "prettier/prettier",
  "funding": "https://github.com/prettier/prettier?sponsor=1",
  "homepage": "https://prettier.io",
  "author": "James Long",
  "type": "module",
  "license": "MIT",
  "main": "./src/index.cjs",
  "browser": "./standalone.js",
  "unpkg": "./standalone.js",
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "require": "./src/index.cjs",
      "default": "./src/index.js"
    },
    "./*": "./*"
  },
  "engines": {
    "node": ">=16"
  },
  "files": [
    "index.js",
    "standalone.js",
    "src",
    "bin"
  ],
  "dependencies": {
    "@angular/compiler": "16.2.12",
    "@babel/code-frame": "7.27.1",
    "@babel/parser": "7.28.0",
    "@babel/types": "7.28.2",
    "@glimmer/syntax": "0.94.9",
    "@iarna/toml": "2.2.5",
    "@prettier/is-es5-identifier-name": "0.2.0",
    "@prettier/parse-srcset": "3.1.0",
    "@typescript-eslint/typescript-estree": "8.38.0",
    "@typescript-eslint/visitor-keys": "8.38.0",
    "acorn": "8.15.0",
    "acorn-jsx": "5.3.2",
    "angular-estree-parser": "13.0.2",
    "angular-html-parser": "9.2.0",
    "camelcase": "8.0.0",
    "chalk": "5.3.0",
    "ci-info": "4.3.0",
    "cjk-regex": "3.3.0",
    "collapse-white-space": "1.0.6",
    "cosmiconfig": "9.0.0",
    "css-units-list": "2.1.0",
    "dashify": "2.0.0",
    "diff": "8.0.2",
    "eastasianwidth": "0.3.0",
    "editorconfig": "3.0.1",
    "editorconfig-to-prettier": "1.0.0",
    "emoji-regex": "10.4.0",
    "escape-string-regexp": "5.0.0",
    "espree": "10.4.0",
    "fast-glob": "3.3.3",
    "fast-json-stable-stringify": "2.1.0",
    "file-entry-cache": "10.1.3",
    "find-cache-dir": "4.0.0",
    "find-parent-dir": "0.3.1",
    "flow-parser": "0.277.1",
    "get-stdin": "9.0.0",
    "graphql": "16.11.0",
    "hermes-parser": "0.30.0",
    "html-element-attributes": "3.4.0",
    "html-styles": "1.0.0",
    "html-tag-names": "2.1.0",
    "ignore": "7.0.5",
    "import-meta-resolve": "4.1.0",
    "jest-docblock": "30.0.1",
    "json5": "2.2.3",
    "leven": "4.0.0",
    "lines-and-columns": "2.0.3",
    "linguist-languages": "7.31.2",
    "mem": "10.0.0",
    "meriyah": "6.1.1",
    "micromatch": "4.0.8",
    "minimist": "1.2.8",
    "n-readlines": "1.0.1",
    "outdent": "0.8.0",
    "please-upgrade-node": "3.2.0",
    "postcss": "8.5.6",
    "postcss-less": "6.0.0",
    "postcss-media-query-parser": "0.2.3",
    "postcss-scss": "4.0.6",
    "postcss-selector-parser": "7.1.0",
    "postcss-values-parser": "7.0.0",
    "regexp-util": "2.0.3",
    "remark-footnotes": "5.0.0",
    "remark-math": "6.0.0",
    "remark-parse": "11.0.0",
    "sdbm": "2.0.0",
    "strip-ansi": "7.1.0",
    "to-fast-properties": "4.0.0",
    "typescript": "5.1.6",
    "unicode-regex": "4.1.2",
    "unified": "11.0.5",
    "vnopts": "2.0.2",
    "wcwidth": "1.0.1",
    "yaml-unist-parser": "2.0.5"
  },
  "devDependencies": {
    "@babel/generator": "7.28.0",
    "@esbuild-plugins/node-modules-polyfill": "0.2.2",
    "@glimmer/reference": "0.94.8",
    "@types/estree": "1.0.1",
    "@types/file-entry-cache": "5.0.2",
    "@types/find-cache-dir": "5.0.2",
    "@types/jest": "30.0.0",
    "@typescript-eslint/eslint-plugin": "8.38.0",
    "benchmark": "2.1.4",
    "browserslist-to-esbuild": "2.1.1",
    "c8": "10.1.3",
    "cross-env": "10.0.0",
    "cspell": "9.2.0",
    "enquirer": "2.3.6",
    "esbuild": "0.18.17",
    "esbuild-visualizer": "0.7.0",
    "eslint": "9.32.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-formatter-friendly": "7.0.0",
    "eslint-plugin-compat": "6.0.2",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jest": "29.0.1",
    "eslint-plugin-n": "17.21.1",
    "eslint-plugin-prettier-internal-rules": "2.0.1",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-regexp": "2.9.0",
    "eslint-plugin-unicorn": "60.0.0",
    "esm-utils": "4.4.2",
    "execa": "7.1.1",
    "jest": "30.0.5",
    "jest-light-runner": "0.7.9",
    "jest-snapshot-serializer-ansi": "2.2.1",
    "jest-snapshot-serializer-raw": "2.0.0",
    "jest-watch-typeahead": "3.0.1",
    "magic-string": "0.30.17",
    "npm-run-all": "4.1.5",
    "path-browserify": "1.0.1",
    "prettier": "3.6.2",
    "pretty-bytes": "7.0.0",
    "rollup-plugin-license": "3.6.0",
    "semver": "7.5.4",
    "serialize-javascript": "6.0.1",
    "snapshot-diff": "0.10.0",
    "tempy": "3.1.0",
    "ts-expect": "1.3.0"
  },
  "resolutions": {
    "trim": "1.0.1"
  },
  "scripts": {
    "prepublishOnly": "echo \"Error: must publish from dist/\" && exit 1",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --runInBand",
    "test:dev-package": "cross-env INSTALL_PACKAGE=1 yarn test",
    "test:dist": "cross-env NODE_ENV=production yarn test",
    "test:dist-standalone": "cross-env TEST_STANDALONE=1 yarn test:dist",
    "test:dist-lint": "eslint --no-eslintrc --no-ignore --no-inline-config --config=./scripts/bundle-eslint-config.cjs \"dist/**/*.{js,cjs,mjs}\"",
    "perf": "yarn && yarn build && cross-env NODE_ENV=production node ./dist/bin/prettier.cjs",
    "perf:inspect": "yarn && yarn build && cross-env NODE_ENV=production node --inspect-brk ./dist/bin/prettier.cjs",
    "perf:benchmark": "yarn perf --debug-benchmark",
    "perf:compare": "./scripts/benchmark/compare.sh",
    "lint": "run-p \"lint:*\"",
    "lint:typecheck": "tsc",
    "lint:eslint": "cross-env EFF_NO_LINK_RULES=true eslint . --format friendly",
    "lint:changelog": "node ./scripts/lint-changelog.js",
    "lint:prettier": "prettier . \"!test*\" --check",
    "lint:spellcheck": "cspell --no-progress --relative --dot --gitignore",
    "lint:deps": "node ./scripts/check-deps.js",
    "fix": "run-s fix:eslint fix:prettier",
    "fix:eslint": "yarn lint:eslint --fix",
    "fix:prettier": "yarn lint:prettier --write",
    "build": "node ./scripts/build/build.js",
    "build:website": "node ./scripts/build-website.js"
  },
  "c8": {
    "reporter": [
      "lcov",
      "text"
    ],
    "all": true,
    "include": [
      "src/**",
      "bin/**"
    ],
    "exclude": [
      "bin/prettier.js",
      "src/standalone.js",
      "src/index.cjs",
      "src/document/debug.js",
      "src/utils/unexpected-node-error.js",
      "src/language-js/types/estree.d.ts",
      "src/**/**/*.d.ts"
    ]
  },
  "browserslist": [
    ">0.5%",
    "not ie 11",
    "not safari 5.1",
    "not op_mini all"
  ],
  "packageManager": "yarn@4.9.2"
}
